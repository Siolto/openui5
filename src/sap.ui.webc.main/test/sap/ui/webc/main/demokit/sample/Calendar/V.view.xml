<mvc:View controllerName="sap.ui.webc.main.sample.Calendar.C" xmlns="sap.ui.webc.main" xmlns:fiori="sap.ui.webc.fiori" xmlns:mvc="sap.ui.core.mvc" height="100%">
    <Toast id="demoToast" duration="1000"></Toast>

    <fiori:Page>
        <fiori:header>
            <fiori:Bar>
                <fiori:middleContent>
                    <Title text="Calendar"></Title>
                </fiori:middleContent>
            </fiori:Bar>
        </fiori:header>

        <fiori:content>
            <Calendar selectedDatesChange="handleSelectedDatesChange">
                <dates>
                    <CalendarDate value="Control value"></CalendarDate>
                    <CalendarDate value="Control value"></CalendarDate>
                    <CalendarDate value="Control value"></CalendarDate>
                </dates>
            </Calendar>
        </fiori:content>
    </fiori:Page>
</mvc:View>